			<token>Year</token>
			<token>#include</token>
		</tokens>
	</tokenset>
	<tokenset name="Strings" id="strings" type="scope" singlelinescope="yes" forecolor="4194432" fontstyle="bold">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
			<token>"</token>
		</tokens>
		<tokens2>
			<token>"</token>
		</tokens2>
	</tokenset>
	<tokenset name="LongStrings" id="longstrings" type="scope" singlelinescope="no" forecolor="4194432" fontstyle="bold">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
			<token>&lt;#</token>
		</tokens>
		<tokens2>
			<token>#&gt;</token>
		</tokens2>
	</tokenset>
	<tokenset name="Escape Sequences" id="escapeseqs" hidden="yes" inheritfrom="Strings">
		<validscope name="Strings"/>
		<tokens>
			<token regexp="yes" rule="">\\.</token>
		</tokens>
	</tokenset>
	<tokenset name="Operators" id="operators" forecolor="blue">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
			<token>^</token>
			<token>-</token>
			<token>*</token>
			<token>/</token>
			<token>\</token>
			<token>+</token>
			<token>&amp;</token>
			<token>=</token>
			<token>&lt;&gt;</token>
			<token>&lt;</token>
			<token>&gt;</token>
			<token>&lt;=</token>
			<token>&gt;=</token>
			<token>Mod</token>
			<token>Is</token>
			<token>And</token>
			<token>Eqv</token>
			<token>Imp</token>
			<token>Not</token>
			<token>Or</token>
			<token>Xor</token>
		</tokens>
	</tokenset>
	<tokenset name="Constants" id="variables" forecolor="8421440" fontstyle="bold">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
			<token>vbBinaryCompare</token>
			<token>vbCr</token>
			<token>vbCrLf</token>
			<token>vbGeneralDate</token>
			<token>vbLf</token>
			<token>vbLongDate</token>
			<token>vbLongTime</token>
			<token>vbObjectError</token>
			<token>vbShortDate</token>
			<token>vbShortTime</token>
			<token>vbTab</token>
			<token>vbTextCompare</token>
		</tokens>
	</tokenset>
	<tokenset name="Numbers" id="numbers">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens regexp="yes">
			<token>&amp;H[a-fA-F0-9]+</token>
			<token>-?\d+(\.\d*)?([eE]-?\d+)?</token>
			<token>&amp;O[0-7]+</token>
		</tokens>
	</tokenset>
	<tokenset name="Text" id="text">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
	</tokenset>
</language>
