			<token>Fix</token>
			<token>FormatCurrency</token>
			<token>FormatDateTime</token>
			<token>FormatNumber</token>
			<token>FormatPercent</token>
			<token>Function</token>
			<token>Get</token>
			<token>GetLocale</token>
			<token>GetObject</token>
			<token>GetRef</token>
			<token>Hex</token>
			<token>Hour</token>
			<token>InputBox</token>
			<token>InStr</token>
			<token>InStrRev</token>
			<token>Int</token>
			<token>If</token>
			<token>In</token>
			<token>IsArray</token>
			<token>IsDate</token>
			<token>IsEmpty</token>
			<token>IsNull</token>
			<token>IsNumeric</token>
