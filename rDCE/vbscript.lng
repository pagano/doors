			<token>RGB</token>
			<token>Right</token>
			<token>Rnd</token>
			<token>Round</token>
			<token>RTrim</token>
			<token>ScriptEngine</token>
			<token>ScriptEngineBuildVersion</token>
			<token>ScriptEngineMajorVersion</token>
			<token>ScriptEngineMinorVersion</token>
			<token>Second</token>
			<token>Select</token>
			<token>Set</token>
			<token>SetLocale</token>
			<token>Sgn</token>
			<token>Sin</token>
			<token>Space</token>
			<token>Split</token>
			<token>Sqr</token>
			<token>Step</token>
