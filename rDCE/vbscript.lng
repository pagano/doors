<language name="VBScript" casesensitive="no" wordchars="[\w]" whitespaceasparamblock="yes">
	<tokenset name="Whitespace" id="whitespace" hidden="yes" inheritfrom="Text">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
			<token regexp="yes">\s+</token>
		</tokens>
	</tokenset>
	<tokenset name="Line Comments" id="linecomments" type="scope" forecolor="green" fontstyle="italic">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
			<token>'</token>
			<token>Rem</token>
		</tokens>
	</tokenset>
	<tokenset name="Scope Keywords" id="scopekeywords" type="scope" autoindent="yes" hidden="yes" inheritfrom="Keywords">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<tokens>
			<token>Begin</token>
			<token>While</token>
			<token>For</token>
			<token>Do</token>
			<token>With</token>
			<token>Function</token>
			<token>Class</token>
			<token>Sub</token>
			<token>Property</token>
			<token>If</token>
			<token>Select Case</token>
		</tokens>
		<tokens2>
			<token>End</token>
			<token>Wend</token>
			<token>Next</token>
			<token>Loop</token>
			<token>End With</token>
			<token>End Function</token>
			<token>End Class</token>
			<token>End Sub</token>
			<token>End Property</token>
			<token>End If</token>
			<token>End Select</token>
		</tokens2>
	</tokenset>
	<tokenset name="Parameter Block" id="parameterblocks" type="scope" hidden="yes" inheritfrom="Text">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
 			<token action="ParameterInfo">(</token>
		</tokens>
		<tokens2>
 			<token>)</token>
		</tokens2>
	</tokenset>
	<tokenset name="Argument Separators" id="argseparators" hidden="yes" inheritfrom="Text">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
 			<token>,</token>
		</tokens>
	</tokenset>
	<tokenset name="Keywords" id="keywords" forecolor="blue" fontstyle="bold">
		<validscope name=""/>
		<validscope name="Scope Keywords"/>
		<validscope name="Parameter Block"/>
		<tokens>
			<token>Abs</token>
			<token>Array</token>
			<token>Asc</token>
			<token>Atn</token>
			<token>ByRef</token>
			<token>ByVal</token>
			<token>Call</token>
			<token>Case</token>
			<token>CBool</token>
			<token>CByte</token>
			<token>CCur</token>
			<token>CDate</token>
			<token>CDbl</token>
			<token>Chr</token>
			<token>CInt</token>
			<token>Class</token>
			<token>CLng</token>
			<token>Const</token>
			<token>Cos</token>
			<token>CreateObject</token>
			<token>CSng</token>
			<token>CStr</token>
			<token>Date</token>
			<token>DateAdd</token>
			<token>DateDiff</token>
			<token>DatePart</token>
			<token>DateSerial</token>
			<token>DateValue</token>
